<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    android:minWidth="25px"
    android:minHeight="25px">
    <LinearLayout
        android:orientation="vertical"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="400dp"
        android:id="@+id/linearLayout1">
        <ListView
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:descendantFocusability="blocksDescendants"
            android:id="@+id/listView1" />
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="fill_parent"
        android:layout_height="60dp"
        android:id="@+id/linearLayout6"
        android:layout_marginBottom="0.0dp">
        <LinearLayout
            android:orientation="vertical"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="150dp"
            android:layout_height="match_parent"
            android:layout_marginTop="5dp"
            android:id="@+id/linearLayout4"
            android:layout_weight="1">
            <ToggleButton
                android:text="标记样车抽取(第一次)"
                android:textOff="标记样车抽取(第一次)"
                android:textOn="标记样车抽取(第一次)"
                android:layout_width="150dp"
                android:layout_height="50dp"
                android:id="@+id/btSamp"
                android:background="#BD2B32"
                android:textColor="#FFFFFF"
                android:layout_marginLeft="10dp" />
        </LinearLayout>
        <LinearLayout
            android:orientation="vertical"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="140dp"
            android:layout_height="match_parent"
            android:layout_marginTop="5dp"
            android:id="@+id/linearLayout4"
            android:layout_weight="1">
            <ToggleButton
                android:text="标记样车抽取(第二次)"
                android:textOff="标记样车抽取(第二次)"
                android:textOn="标记样车抽取(第二次)"
                android:layout_width="140dp"
                android:layout_height="50dp"
                android:id="@+id/btSamp2"
                android:background="#BD2B32"
                android:textColor="#FFFFFF"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="20dp" />
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:orientation="vertical"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:id="@+id/linearLayout1">
        <ToggleButton
            android:text="生过烧标记保存"
            android:textOff="生过烧标记保存"
            android:textOn="生过烧标记保存"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:id="@+id/btSampExtra"
            android:background="#3F91DA"
            android:textColor="#FFFFFF"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="20dp" />
    </LinearLayout>
</LinearLayout>