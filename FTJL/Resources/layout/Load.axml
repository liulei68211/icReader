<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white">
  <LinearLayout
      android:orientation="horizontal"
      android:minWidth="25px"
      android:minHeight="25px"
      android:layout_width="fill_parent"
      android:layout_height="400.0dp"
      android:id="@+id/linearLayout1"
      android:layout_marginBottom="0.0dp">
    <LinearLayout
        android:orientation="vertical"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/linearLayout2"
        android:layout_weight="1"
        android:layout_marginBottom="0.0dp">
      <TextView
          android:text="车辆信息"
          android:layout_width="match_parent"
          android:layout_height="30dp"
          android:textStyle="normal"
          android:layout_marginTop="8dp"
          android:textSize="20dp"
          android:textColor="#0D1217"
          android:layout_marginLeft="0px"
          android:id="@+id/textView1" />
      <TextView
          android:text="业务类型"
          android:layout_width="match_parent"
          android:layout_height="30dp"
          android:textStyle="normal"
          android:layout_marginTop="12dp"
          android:textSize="20dp"
          android:textColor="#0D1217"
          android:id="@+id/textView2"
          android:layout_marginLeft="0px" />
      <TextView
          android:text="计量类型"
          android:layout_width="match_parent"
          android:layout_height="30dp"
          android:textStyle="normal"
          android:layout_marginTop="12dp"
          android:textSize="20dp"
          android:textColor="#0D1217"
          android:id="@+id/textView3"
          android:layout_marginLeft="0px" />
      <TextView
          android:text="计重方式"
          android:layout_width="match_parent"
          android:layout_height="30dp"
          android:textStyle="normal"
          android:layout_marginTop="13dp"
          android:textSize="20dp"
          android:textColor="#0D1217"
          android:id="@+id/textView4"
          android:layout_marginLeft="0px" />
      <TextView
          android:text="计量单号"
          android:layout_width="match_parent"
          android:layout_height="30dp"
          android:textStyle="normal"
          android:layout_marginTop="14dp"
          android:textSize="20dp"
          android:textColor="#0D1217"
          android:id="@+id/textView5"
          android:layout_marginLeft="0px" />
      <TextView
          android:text="货物重量"
          android:layout_width="match_parent"
          android:layout_height="30dp"
          android:textStyle="normal"
          android:layout_marginTop="14dp"
          android:textSize="20dp"
          android:textColor="#0D1217"
          android:id="@+id/textView9"
          android:layout_marginLeft="0px" />
      <TextView
          android:text="操作时间"
          android:layout_width="match_parent"
          android:layout_height="30dp"
          android:textStyle="normal"
          android:layout_marginTop="14dp"
          android:textSize="20dp"
          android:textColor="#0D1217"
          android:id="@+id/textView9"
          android:layout_marginLeft="0px" />
      <TextView
          android:text="登录人员"
          android:layout_width="match_parent"
          android:layout_height="30dp"
          android:textStyle="normal"
          android:layout_marginTop="14dp"
          android:textSize="20dp"
          android:textColor="#0D1217"
          android:id="@+id/textView10"
          android:layout_marginLeft="0px" />
      <TextView
          android:text="扣吨:公斤"
          android:layout_width="match_parent"
          android:layout_height="30dp"
          android:textStyle="bold"
          android:layout_marginTop="14dp"
          android:textSize="20dp"
          android:id="@+id/textView11"
          android:layout_marginLeft="0px"
          android:textColor="@android:color/holo_red_dark" />
    </LinearLayout>
    <LinearLayout
        android:orientation="vertical"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/linearLayout4"
        android:layout_weight="0.4">
      <EditText
          android:layout_width="match_parent"
          android:layout_height="38dp"
          android:layout_marginTop="5dp"
          android:textSize="20dp"
          android:paddingBottom="1dip"
          android:textColor="@android:color/black"
          android:maxLines="1"
          android:id="@+id/tuCarInfo"
          android:enabled="false" />
      <EditText
          android:layout_width="match_parent"
          android:layout_height="38dp"
          android:layout_marginTop="5dp"
          android:textSize="20dp"
          android:paddingBottom="1dip"
          android:textColor="@android:color/black"
          android:maxLines="1"
          android:id="@+id/tuBussnise"
          android:enabled="false" />
      <EditText
          android:layout_height="38dp"
          android:layout_marginTop="5dp"
          android:textSize="20dp"
          android:paddingBottom="1dip"
          android:textColor="@android:color/black"
          android:layout_width="match_parent"
          android:id="@+id/tuMeasureType"
          android:enabled="false"
          android:maxLines="1" />
      <EditText
          android:layout_height="38dp"
          android:layout_marginTop="5dp"
          android:textSize="20dp"
          android:paddingBottom="1dip"
          android:textColor="@android:color/black"
          android:layout_width="match_parent"
          android:id="@+id/tuMeasureMode"
          android:enabled="false"
          android:maxLines="1" />
      <EditText
          android:layout_height="38dp"
          android:layout_marginTop="5dp"
          android:textSize="20dp"
          android:paddingBottom="1dip"
          android:textColor="@android:color/black"
          android:layout_width="match_parent"
          android:id="@+id/tuMeasureID"
          android:enabled="false"
          android:maxLines="1" />
      <EditText
          android:layout_height="38dp"
          android:layout_marginTop="5dp"
          android:textSize="20dp"
          android:paddingBottom="1dip"
          android:textColor="@android:color/black"
          android:layout_width="match_parent"
          android:id="@+id/tuGoodWeight"
          android:enabled="false"
          android:maxLines="1" />
      <EditText
          android:layout_height="38dp"
          android:layout_marginTop="5dp"
          android:textSize="20dp"
          android:paddingBottom="1dip"
          android:textColor="@android:color/black"
          android:layout_width="match_parent"
          android:id="@+id/tuMeasureDT"
          android:enabled="false"
          android:maxLines="1" />
      <EditText
          android:layout_height="38dp"
          android:layout_marginTop="5dp"
          android:textSize="20dp"
          android:paddingBottom="1dip"
          android:textColor="@android:color/black"
          android:layout_width="match_parent"
          android:id="@+id/tuPerson"
          android:enabled="false"
          android:maxLines="1" />
      <EditText
          android:layout_height="38dp"
          android:layout_marginTop="5dp"
          android:textSize="20dp"
          android:paddingBottom="1dip"
          android:layout_width="match_parent"
          android:id="@+id/eBuckleWeight"
          android:inputType="number"
          android:text="0"
          android:textColor="@android:color/holo_red_dark"
          android:maxLines="1"
          android:maxLength="8"
          android:clickable="true"
          android:focusableInTouchMode="true" />
    </LinearLayout>
  </LinearLayout>
  <LinearLayout
      android:orientation="horizontal"
      android:minWidth="25px"
      android:minHeight="25px"
      android:layout_width="fill_parent"
      android:layout_height="match_parent"
      android:id="@+id/linearLayout6"
      android:layout_marginBottom="0.0dp">
    <LinearLayout
        android:orientation="vertical"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/linearLayout4"
        android:layout_weight="1"
        android:layout_marginTop="0.0dp">
      <ToggleButton
          android:text="确认装货"
          android:textOff="确认装货"
          android:textOn="确认装货"
          android:layout_width="210px"
          android:layout_height="65px"
          android:id="@+id/buUnLoad"
          android:layout_marginTop="20px"
          android:layout_marginLeft="10px" />
      <ToggleButton
          android:text="半车退货"
          android:textOff="半车退货"
          android:textOn="半车退货"
          android:layout_width="210px"
          android:layout_height="65px"
          android:id="@+id/buHalfReturn"
          android:layout_marginTop="20px"
          android:layout_marginLeft="10px" />
    </LinearLayout>
    <LinearLayout
        android:orientation="vertical"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/linearLayout5"
        android:layout_weight="1">
      <ToggleButton
          android:text="全车退货"
          android:textOff="全车退货"
          android:textOn="全车退货"
          android:layout_width="210px"
          android:layout_height="65px"
          android:id="@+id/buAllReturn"
          android:layout_marginLeft="10px"
          android:layout_marginTop="20px" />
      <ToggleButton
          android:text="妆后退货"
          android:textOff="妆后退货"
          android:textOn="妆后退货"
          android:layout_width="210px"
          android:layout_height="65px"
          android:id="@+id/buUnloadAndLoad"
          android:layout_marginTop="20px"
          android:layout_marginLeft="10px" />
    </LinearLayout>
  </LinearLayout>
</LinearLayout>